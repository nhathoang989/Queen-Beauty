@**@
<!DOCTYPE html>
<html lang="en">

<head>
    <base href="/portal">
    <meta charset="utf-8" />
    <link rel="apple-touch-icon" sizes="76x76" href="~/lib/now-ui-kit-pro-html-v1.2.0/assets/img/apple-icon.png">
    <link rel="icon" type="image/png" href="~/lib/now-ui-kit-pro-html-v1.2.0/assets/img/favicon.png">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <title>@ViewData["Title"] - Swastika I/O</title>
    <meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0, shrink-to-fit=no' name='viewport' />
    <!--     Fonts and icons     -->
    <link href="https://fonts.googleapis.com/css?family=Montserrat:400,700,200" rel="stylesheet" />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css" />
    <environment names="Development,Staging,Production">
        <!-- CSS Files -->
        <link href="~/lib/now-ui-kit-pro-html-v1.2.0/assets/css/bootstrap.min.css" rel="stylesheet" />
        <link href="~/lib/now-ui-kit-pro-html-v1.2.0/assets/css/now-ui-kit.css" rel="stylesheet" />
        <!-- CSS Just for demo purpose, don't include it in your project -->
        <link href="~/lib/now-ui-kit-pro-html-v1.2.0/assets/css/demo.css" rel="stylesheet" />
    </environment>
    <link href="~/css/portal/main.css" rel="stylesheet" />
    @*@Html.Raw(JavaScriptSnippet.FullScript)*@
</head>

<body class="login-page" ng-app="SwastikaPortal">
    <div class="loader-container" ng-show="isBusy">
        <div id="loader"></div>
    </div>
    <!-- End Navbar -->
    <div class="page-header" filter-color="purple">
        <div class="page-header-image" style="background-image:url(/images/bgs/book-open.jpeg)"></div>
        <div class="content">
        <div class="container">
            @RenderBody()
            </div>
        </div>
        <footer class="footer">
            <div class="container">
                <div class="copyright">
                    &copy;
                    <script>
                        document.write(new Date().getFullYear())
                    </script>
                    <a href="http://www.swastika.io" target="_blank">Swastika I/O</a>.
                </div>
            </div>
        </footer>
    </div>
    <environment names="Development">
        <!--   Core JS Files   -->
        <script src="~/lib/now-ui-kit-pro-html-v1.2.0/assets/js/core/jquery.3.2.1.min.js" type="text/javascript"></script>
        @*<script src="~/lib/now-ui-kit-pro-html-v1.2.0/assets/js/core/tether.min.js" type="text/javascript"></script>*@
        <script src="~/lib/now-ui-kit-pro-html-v1.2.0/assets/js/core/bootstrap.min.js" type="text/javascript"></script>
        <!--  Plugin for Switches, full documentation here: http://www.jque.re/plugins/version3/bootstrap.switch/ -->
        <script src="~/lib/now-ui-kit-pro-html-v1.2.0/assets/js/plugins/bootstrap-switch.js"></script>
        <!--  Plugin for the Sliders, full documentation here: http://refreshless.com/nouislider/ -->
        <script src="~/lib/now-ui-kit-pro-html-v1.2.0/assets/js/plugins/nouislider.min.js" type="text/javascript"></script>
        <!--  Plugin for the DatePicker, full documentation here: https://github.com/uxsolutions/bootstrap-datepicker -->
        @*<script src="~/lib/now-ui-kit-pro-html-v1.2.0/assets/js/plugins/bootstrap-datepicker.js" type="text/javascript"></script>*@
        <!-- Control Center for Now Ui Kit: parallax effects, scripts for the example pages etc -->
        <script src="~/lib/now-ui-kit-pro-html-v1.2.0/assets/js/now-ui-kit.js" type="text/javascript"></script>
    </environment>

    <script src="~/js/angular.min.js"></script>
    <script src="//ajax.googleapis.com/ajax/libs/angularjs/1.2.16/angular-route.min.js"></script>
    <script src="//ajax.googleapis.com/ajax/libs/angularjs/1.2.13/angular-animate.js"></script>
    <script src="~/js/plugins/ng-file-upload-bower-12.2.13/ng-file-upload-all.js"></script>
    <script src="~/js/plugins/ng-file-upload-bower-12.2.13/ng-file-upload-shim.js"></script>
    <script src="/app-init/plugins/angular-local-storage.min.js"></script>
    <script src="~/app-init/app.js"></script>
    <script src="~/app-init/app.route.js"></script>
    <script src="~/app-init/pages/step1/step1-controller.js"></script>
    <script src="~/app-init/pages/step1/step1-services.js"></script>
    
    <script src="~/app-init/pages/step2/step2-controller.js"></script>
    <script src="~/app-init/pages/step2/step2-services.js"></script>
    <script src="~/app-init/pages/shared/services/common-services.js"></script>

    @RenderSection("Scripts", required: false)
</body>

</html>
