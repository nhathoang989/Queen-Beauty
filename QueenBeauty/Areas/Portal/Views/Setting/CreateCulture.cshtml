@model Swastika.Cms.Lib.ViewModels.CultureViewModel
@{
    ViewData["Title"] = Swastika.Cms.Lib.Services.GlobalConfigurationService.Instance.GetLocalString("Create Culture",ViewBag.culture);
    //Layout = "~/Areas/Portal/Views/Shared/_Layout.cshtml";
}

<section class="content container-fluid">
    <form asp-controller="@ViewContext.RouteData.Values["controller"].ToString()"
          asp-action="@ViewContext.RouteData.Values["action"].ToString()">
        <div class="row">

            <section class="col-lg-9">
                @await Html.PartialAsync("_Editable_Culture", Model)
            </section>
            <section class="col-lg-3">
                @await Html.PartialAsync("EditableSides/_EditableSide_Actions", Model)
            </section>

        </div>
    </form>
</section>